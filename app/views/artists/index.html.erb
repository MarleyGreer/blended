<div class="container">
  <h1>Artists Index  Page </h1>
 <% @artists.each do |artist| %>
    <%= cl_image_tag artist.user.photo.key, height: 150, width: 150, gravity: :face, crop: :fill, radius: :max %>
    <h2><%= artist.user.first_name %> <%= artist.user.last_name %></h2>
    <h2><%= artist.description %></h2>
    <%= link_to "View", artist_path(artist)  %>
 <% end %>
</div>

